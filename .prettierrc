{
  "semi": true,
  "printWidth": 100,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "vueIndentScriptAndStyle": true,
  "htmlWhitespaceSensitivity": "ignore",
  "endOfLine": "lf",
  "plugins": ["prettier-plugin-tailwindcss"],
  "useTabs": false,
  "tabWidth": 2,
  "singleQuote": false,
  "proseWrap": "never"
}
